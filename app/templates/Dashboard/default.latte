{block content}
	<div class="panel panel-primary">
		<div class="panel-heading">
			Next run
		</div>
		<div class="panel-body">
			{$nextRunTime}
		</div>
	</div>
	<div class="panel panel-info">
		<div class="panel-heading">
			Queue
		</div>
		<div class="panel-body">
			<ol>
				<li n:foreach="$queue as $command">
					{control displayCommand, $command}
				</li>
			</ol>
		</div>
	</div>
{/block}
