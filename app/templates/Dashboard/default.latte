{block content}
	<a n:href="runBot" class="btn btn-warning">Run bot</a>
	<div class="panel panel-primary">
		<div class="panel-heading">
			Next run
		</div>
		<div class="panel-body">
			{$nextRunTime}
		</div>
	</div>
	<div class="panel panel-info">
		<div class="panel-heading">
			Queue
		</div>
		<div class="panel-body dashboard-queue" n:inner-foreach="$queue as $command">
			{control displayCommand, $iterator, $command}
		</div>
	</div>
{/block}
